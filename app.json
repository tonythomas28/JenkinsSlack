{
  "name": "Jenkins Slack Integration App",
  "description": "An instance for Jenkins Slack Command to run Normal and Parametrised Builds",
  "repository": "https://github.com/tonythomas28/JenkinsSlack",
  "keywords": ["jenkins", "slack", "command"],
  "env": {
    "SLACK_TOKEN": {
      "description": "Token from your 'Slash Command'",
      "required": true
    },
    "SLACK_WEBHOOK_URL": {
      "description": "Your incoming webhook URL"
    },
    "JENKINS_URL": {
      "description": "URL to your Heroku instnace",
      "required": true
    },
    "JENKINS_TOKEN": {
      "description": "API token of your user in Jenkins",
      "required": true
    }
  }
}
